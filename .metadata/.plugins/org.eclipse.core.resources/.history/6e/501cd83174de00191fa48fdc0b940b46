package ch4;

import java.applet.Applet; 
import java.awt.Graphics;

final class Pc {
	static double ad = 8;
}

public class C4_7 extends Applet {
	public void paint(Graphics g) {
		P1 n = new P1();
		n.f(g);
		n.i = 40;	n.j++;
		g.drawString("i="+n.i, 20, 70);
		g.drawString("j="+n.j, 20, 90);
	}
}