package ch3;

public class C3_exercises {
	static void test_3(int x) {
		int y = 0;
		if (x<1) {
			y = x;
		} else if (x<10) {
			y = 3 * x - 2;
		} else {
			y = 4 * x;
		}
		System.out.print(y);
//		return y + "12";
	}
	
	void test_6() {
		for (int k=1; k<=5; k++) {
			if(k>4) {
				break;
			}
			System.out.println("k=" + k);
		}
	}
	
	public static void main(String[] args) {
		test_3(12);
	}
}
